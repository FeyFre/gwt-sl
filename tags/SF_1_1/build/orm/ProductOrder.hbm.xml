<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.gwtwidgets.server.spring.test.domain">

	<class name="ProductOrder" table="ProductOrder">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		
		<list name="products" table="ORDER_PRODUCTS" lazy="false">
			<key column="orderId"></key>
			<list-index base="0" column="productIndex"/>
			<many-to-many column="productId" class="Product"/>
		</list>
	</class>

</hibernate-mapping>